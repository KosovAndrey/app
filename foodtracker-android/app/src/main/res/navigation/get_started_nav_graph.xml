<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/get_started_nav_graph"
    app:startDestination="@id/nameFragment">

    <fragment
        android:id="@+id/nameFragment"
        android:name="com.foodtracker.app.presentation.getstarted.name.NameFragment"
        android:label="fragment_name"
        tools:layout="@layout/fragment_name" >
        <action
            android:id="@+id/action_nameFragment_to_goalFragment"
            app:destination="@id/goalFragment" />
    </fragment>
    <fragment
        android:id="@+id/goalFragment"
        android:name="com.foodtracker.app.presentation.getstarted.goal.GoalFragment"
        android:label="fragment_goal"
        tools:layout="@layout/fragment_goal" >
        <action
            android:id="@+id/action_goalFragment_to_genderFragment"
            app:destination="@id/genderFragment" />
    </fragment>
    <fragment
        android:id="@+id/genderFragment"
        android:name="com.foodtracker.app.presentation.getstarted.gender.GenderFragment"
        android:label="fragment_gender"
        tools:layout="@layout/fragment_gender" >
        <action
            android:id="@+id/action_genderFragment_to_birthdayFragment"
            app:destination="@id/birthdayFragment" />
    </fragment>
    <fragment
        android:id="@+id/birthdayFragment"
        android:name="com.foodtracker.app.presentation.getstarted.birthday.BirthdayFragment"
        android:label="fragment_birthday"
        tools:layout="@layout/fragment_birthday" >
        <action
            android:id="@+id/action_birthdayFragment_to_heightFragment"
            app:destination="@id/heightFragment" />
    </fragment>
    <fragment
        android:id="@+id/heightFragment"
        android:name="com.foodtracker.app.presentation.getstarted.height.HeightFragment"
        android:label="fragment_height"
        tools:layout="@layout/fragment_height" >
        <action
            android:id="@+id/action_heightFragment_to_activeFragment"
            app:destination="@id/activeFragment" />
    </fragment>
    <fragment
        android:id="@+id/activeFragment"
        android:name="com.foodtracker.app.presentation.getstarted.active.ActiveFragment"
        android:label="fragment_active"
        tools:layout="@layout/fragment_active" >
        <action
            android:id="@+id/action_activeFragment_to_currentWeightFragment"
            app:destination="@id/currentWeightFragment" />
    </fragment>
    <fragment
        android:id="@+id/currentWeightFragment"
        android:name="com.foodtracker.app.presentation.getstarted.currentWeight.CurrentWeightFragment"
        android:label="fragment_current_weight"
        tools:layout="@layout/fragment_current_weight" >
        <action
            android:id="@+id/action_currentWeightFragment_to_goalWeightFragment"
            app:destination="@id/goalWeightFragment" />
    </fragment>
    <fragment
        android:id="@+id/goalWeightFragment"
        android:name="com.foodtracker.app.presentation.getstarted.goalWeight.GoalWeightFragment"
        android:label="fragment_goal_weight"
        tools:layout="@layout/fragment_goal_weight" >
        <action
            android:id="@+id/action_goalWeightFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.foodtracker.app.presentation.register.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
</navigation>